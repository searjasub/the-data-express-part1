extends header

block content
    table(class='gridtable')
        tr
            th Id
            th Username
            th Age
            th Email
            th
            th
        each item in people
            tr
                td
                    a(href='/details/' + item.id) #{item.id}
                td #{item.username}
                td #{item.age}
                td #{item.email}
                td
                    a(href='/edit/' + item.id) Edit
                td
                    a(href='/delete/' + item.id) Delete